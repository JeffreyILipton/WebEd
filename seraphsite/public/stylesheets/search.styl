#guide
   display table
   width 100%
   
.logo
   display table-cell
   width 10%
   img
     width 80px
     height 80px

   
.guidebar
  display table-cell
  width 70%

.accountDiv
  display table-cell
  width 20%  
  
  
  
  
.searchbar
  width 100%
  text-align center
  float left
  display inline
  background #000
  color #fff
  
  div 
    display inline
    float left
    padding 0 20px 0 20px  

  .label
    margin-top 5px

    
.breadcrumbs
    width 100%
    display block
    background #262626
    left 0  
    height 40px
    margin 0px
    padding-top 5px
    padding-bottom 5px
    margin-top 5px
    margin-bottom 5px
    
 
    ul 
       display inline    
       margin 0 auto  
       list-style none  
  
       li   
          float left
          padding-left 10px
          margin-top 5px
          
          a
            display inline
            height 30px
            margin-right 20px
            margin-top 5px 
       	    padding-right 15px
            font-size 14px 
       	    text-align center
       	    text-decoration none
       	    color #e6e6e6
       	    &:hover
       	      color #f5f

    .breadButtons
        display inline
         padding-left 10px